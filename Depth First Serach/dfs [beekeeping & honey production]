#include <iostream>
using namespace std;

int sections = 5; // Number of gym sections
int gym[10][10] = {
    {0,1,1,0,0},  // Section 0 connected to 1,2
    {1,0,0,1,1},  // Section 1 connected to 0,3,4
    {1,0,0,0,1},  // Section 2 connected to 0,4
    {0,1,0,0,0},  // Section 3 connected to 1
    {0,1,1,0,0}   // Section 4 connected to 1,2
};
int visited[10]; 

// DFS traversal to inspect gym sections in detail
void dfs(int gym[10][10], int sections, int startSection) {
    visited[startSection] = 1;
    
    for (int i = 0; i < sections; i++) {
        if (gym[startSection][i] == 1 && visited[i] == 0) {
            cout << "Section " << i << " -> ";
            dfs(gym, sections, i);
        }
    }
}

int main() {
    int startSection;
    for (int i = 0; i < sections; i++)
        visited[i] = 0;

    cout << "Enter the starting section for detailed inspection: ";
    cin >> startSection;

    cout << "Detailed DFS Inspection of Gym Sections...\n";
    cout << "Section " << startSection << " -> ";
    dfs(gym, sections, startSection);

    cout << "End of DFS inspection.\n";
    return 0;
}
